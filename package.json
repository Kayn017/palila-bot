{
	"name": "palila-bot",
	"version": "2.0.0",
	"description": "Le palila bot est un bot super cool",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/Kayn017/palila-bot.git"
	},
	"scripts": {
		"start": "node index.js",
		"build": "eslint . && docker build . -t ",
		"lint": "eslint .",
		"prepare": "husky install",
		"dev": "nodemon index.js --DEV",
		"make:model": "node ./scripts/factory.js MODEL",
		"make:command": "node ./scripts/factory.js COMMAND",
		"make:module": "node ./scripts/factory.js MODULE",
		"make:route": "node ./scripts/factory.js ROUTE",
		"make:worker": "node ./scripts/factory.js WORKER",
		"dev:cleancmd": "node ./scripts/commands.js CLEAN_COMMANDS",
		"dev:updatecache": "node ./scripts/cache.js",
		"add:god": "node ./scripts/gods.js ADD_GOD"
	},
	"author": "Tanguy Gimenez",
	"license": "ISC",
	"dependencies": {
		"@discordjs/opus": "^0.7.0",
		"@discordjs/voice": "^0.8.0",
		"@twurple/api": "^5.1.6",
		"@twurple/auth": "^5.1.6",
		"axios": "^0.24.0",
		"bufferutil": "^4.0.5",
		"discord.js": "13.8.1",
		"dotenv": "^10.0.0",
		"erlpack": "discord/erlpack",
		"express": "^4.17.1",
		"fluent-ffmpeg": "^2.1.2",
		"libsodium-wrappers": "^0.7.9",
		"mariadb": "^3.0.0",
		"mime-types": "^2.1.35",
		"pg-hstore": "^2.3.4",
		"redis": "^4.0.1",
		"rss-parser": "^3.12.0",
		"sequelize": "^6.6.5",
		"soundcloud-downloader": "^1.0.0",
		"twitch-m3u8": "^1.1.5",
		"utf-8-validate": "^5.0.7",
		"ytdl-core": "^4.11.0",
		"zip-a-folder": "^1.1.0",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"eslint": "^7.32.0",
		"husky": "^7.0.4",
		"nodemon": "^2.0.7"
	},
	"engines": {
		"node": "16"
	}
}
